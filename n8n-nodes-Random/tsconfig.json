{
  "compilerOptions": {
    "strict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2019",
    "lib": ["es2019"],
    "removeComments": true,
    "useUnknownInCatchVariables": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": false,
    "noEmitOnError": true,
    "incremental": true,
    "skipLibCheck": true,
    "outDir": "dist/nodes/Random",
    "rootDir": "./nodes/Random"
  },
  "include": [
  
    "nodes/**/*.ts",
    "nodes/**/*.json",

  ],

  "exclude": [
    "node_modules", 
    "dist"
   
  ]
}
